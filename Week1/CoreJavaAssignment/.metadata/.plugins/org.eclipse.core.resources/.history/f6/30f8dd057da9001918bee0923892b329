package Assignment.Java;

public class JavaQ1BubbleSort {

	public static void bubbleSort(int[] arr) {
		boolean swapped = true;
		while (swapped)
			swapped = false;
		int temp = 0;
		for(int i=0; i<arr.length-1; i++) {
			for(int j=1; j<(arr.length); j++) {
				if(arr[j-1] > arr[j]) {
					temp = arr[j];
					arr[j-1] = arr[j];
					arr[j-1] = temp;
					swapped = true;
					
				}
			}
	
		
			
					
					if(swapped == false)
						break;
		
	}
	}
}

	

		
	
