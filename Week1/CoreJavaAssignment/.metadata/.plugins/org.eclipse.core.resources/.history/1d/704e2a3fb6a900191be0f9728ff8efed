package Assignment.Java;

public class JavaQ2Fibonacci {

	static int fib(int n) 
	{
		
	
	if (n <= 1)
		return n;
	return fib(n-1) + fib(n-2);
	}
		public static void main(String[] args) {
	int n = 25, x1 = 0, x2 = 1;
	System.out.println("Up to " + 25 + ": ");
	while (x1 <= n)
	{
		System.out.println(x1 + " +");
		int sum= x1 + x2;
		x1 =x2;
		x2 =sum;
	}
		}
}
