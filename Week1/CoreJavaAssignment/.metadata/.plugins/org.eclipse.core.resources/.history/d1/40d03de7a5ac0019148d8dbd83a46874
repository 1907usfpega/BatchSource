package Assignment.Java;

public class JavaQ7Comparator implements Comparable<JavaQ7Comparator> {
	
    private String name;
    private String department;
    private int age;
 
    public JavaQ7Comparator(int id, String nameFirst, String nameLast, int age) {
    	
        this.name = name;
        this.department = department;
        this.age = age;
    
    }
 
    // get and set
 
    @Override
    public String toString() {
        return "Employee : " + "name=  " + "department= " + "age= ";
    }

}


