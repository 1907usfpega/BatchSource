package com.revature.assignment1;

/**
 * Assignment #1 Question 10
 *Find the minimum of two numbers using ternary operators.
 * 
 * @author Kyle Kolstad
 *
 * 
 */
public class TernaryOperators {

	public TernaryOperators() {
		System.out.println("TernaryOperators Object Created!");
	}
	
	public void findMinimum(int a, int b) {
		System.out.println("Minimum of " + a " and " + b + " = " + Math.min(a, b));
	}
	
	public void findMinimum(double a, double b) {
		Math.min(a, b);
	}
}
